<odoo>
  <data>

    <record id="view_product_template_form_part_print" model="ir.ui.view">
      <field name="name">product.template.part_print</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="product.product_template_only_form_view"/>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <field name="uom_po_id" position="after">
          <field name="part_print"/>
        </field>
      </field>
    </record>

    <record id="view_manufacturing_order_form_part_print" model="ir.ui.view">
      <field name="name">mrp.production.part_print</field>
      <field name="model">mrp.production</field>
      <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <xpath expr="//notebook/page[last()]" position="after">
          <page string="Part Print" name="part_print">
            <group>
              <field name="part_print" string="" widget="pdf_viewer"/>
            </group>
          </page>
        </xpath>
      </field>
    </record>

    <record id="view_mrp_bom_form_part_print" model="ir.ui.view">
      <field name="name">mrp.bom.part_print</field>
      <field name="model">mrp.bom</field>
      <field name="inherit_id" ref="mrp.mrp_bom_form_view"/>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <field name="type" position="after">
          <field name="part_print"/>
        </field>
      </field>
    </record>

