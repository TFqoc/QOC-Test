        <record id="view_procurement_form" model="ir.ui.view">
      <field name="name">procurement.form</field>
      <field name="model">delayed.procurement</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="Procurement">
          <header>
          </header>
          <sheet>
            <group>
              <field name="rule"/>
              <field name="product_id"/>
              <field name="product_qty"/>
              <field name="product_uom"/>
              <field name="location_id"/>
              <field name="name"/>
              <field name="origin"/>
              <field name="company_id"/>
              <field name="values"/>
            </group>
            <group>

            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record model="ir.ui.view" id="view_delayed_procurement_list">
      <field name="name">delayed.procurement.view</field>
      <field name="model">delayed.procurement</field>
      <field name="type">tree</field>
      <field name="arch" type="xml">
        <tree string="Procurement">
          <field name="rule"/>
          <field name="product_id"/>
          <field name="product_qty"/>
          <field name="product_uom"/>
        </tree>
      </field>
    </record>

    <record model="ir.actions.act_window" id="plm2_action_window">
      <field name="name">Delayed_Procurement</field>
      <field name="res_model">delayed.procurement</field>
      <field name="view_mode">list,form</field>
    </record>

    <menuitem id="procurement_root" name="Procurement" sequence="10" action="plm2_action_window"/>

    <menuitem action="stock.action_rules_form" id="menu_action_rules_form2"/>

    <record id="view_mrp_bom_inherit" model="ir.ui.view">
      <field name="name">mrp_bom.inherit</field>
      <field name="model">mrp.bom</field>
      <field name="inherit_id" ref="mrp.mrp_bom_form_view"/>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <field name="type" position="after">
          <field name="previous_bom_id"/>
          <field name="revision_ids"/>
          <field name="version"/>
        </field>
      </field>
    </record>
