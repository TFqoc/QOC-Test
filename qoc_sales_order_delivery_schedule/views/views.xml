      <field name="arch" type="xml">
        <dashboard>
          <group string="Test">
            <view type="list"/>
          </group>
        </dashboard>
      </field>
    </record>

    <record model="ir.ui.view" id="qoc_sales_order_delivery_schedule.qweb">
      <field name="name">qoc_sales_order_delivery_schedule qweb</field>
      <field name="model">sale.order.line</field>
      <field name="arch" type="xml">
        <qweb>
          <field name="id"/>
          <templates>
            <t t-foreach="records" t-as="record">
              <p t-esc="record.id"/>
              <p t-esc="record.name"/>
            </t>
          </templates>
        </qweb>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="qoc_sales_order_delivery_schedule.action_window">
      <field name="name">qoc_sales_order_delivery_schedule window</field>
      <field name="res_model">sale.order.line</field>
      <field name="view_mode">qweb,tree,form,dashboard</field>
    </record>


