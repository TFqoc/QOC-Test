<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="qoc_sales_order_delivery_schedule.list">
      <field name="name">Sales Order Delivery Schedule list</field>
      <field name="model">sale.order.line</field>
      <field name="arch" type="xml">
        <tree>
          <field name="id" />
          <field name="name" />
          <!-- <groupby name="partner_id">
            <field name="name"/>
              <button type="edit" name="edit" string="Edit"/>
              <button type="object" name="my_method" string="Button1"
                attrs="{'invisible': [('name', '=', 'Georges')]}"/>
          </groupby> -->
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="qoc_sales_order_delivery_schedule.dash">
      <field name="name">Sales Order Delivery Schedule dashboard</field>
      <field name="model">sale.order.line</field>
      <field name="arch" type="xml">
        <dashboard>
          <group string="Test">
            <view type="list" />
          </group>
        </dashboard>
      </field>
    </record>

    <record model="ir.ui.view" id="qoc_sales_order_delivery_schedule.qweb">
      <field name="name">Sales Order Delivery Schedule qweb</field>
      <field name="model">sale.order</field>
      <field name="arch" type="xml">
        <qweb>
          <div class="o_list_view">
            <div class="table-responsive">
              <table class="o_list_table table table-sm table-hover table-striped o_list_table_ungrouped" style="table-layout: fixed;">
                <thead>
                  <tr>
                    <th class="o_column_sortable">
                      ID
                      <span class="o_resize" />
                    </th>
                    <th class="o_column_sortable">
                      Name
                      <span class="o_resize" />
                    </th>
                  </tr>
                </thead>
                <tbody>
                  <t t-foreach="records" t-as="record">
                    <tr>
                      <td t-esc="record.id" />
                      <td t-esc="record.name" />
                      <t t-foreach="record.order_line" t-as="so_line">
                        <!-- Manufacturing Orders -->
                        <t t-set="mo_lines" t-value="so_line.get_mo_records()" />
                        <t t-if="mo_lines">
                          <tr>
                            <td colspan="2">
                              <table class="o_list_table table table-sm table-hover table-striped o_list_table_ungrouped" style="table-layout: fixed;">
                                <thead>
                                  <tr>
                                    <th>
                                      MO Name
                                    </th>
                                  </tr>
                                </thead>
                                <tbody>
                                  <t t-foreach="mo_lines" t-as="mo_line">
                                    <tr>
                                      <td t-esc="mo_line.name" />
                                    </tr>
                                  </t>
                                </tbody>
                              </table>
                            </td>
                          </tr>
                        </t>
                        <!-- Purchase Orders -->
                        <t t-if="so_line.purchase_line_ids">
                          <tr>
                            <td colspan="2">
                              <table class="o_list_table table table-sm table-hover table-striped o_list_table_ungrouped" style="table-layout: fixed;">
                                <thead>
                                  <tr>
                                    <th>
                                      PO Name
                                    </th>
                                  </tr>
                                </thead>
                                <tbody>
                                  <t t-foreach="so_line.purchase_line_ids" t-as="purchase_line">
                                    <tr>
                                      <td t-esc="purchase_line.name" />
                                    </tr>
                                  </t>
                                </tbody>
                              </table>
                            </td>
                          </tr>
                        </t>
                      </t>
                    </tr>
                  </t>
                </tbody>
              </table>
            </div>
          </div>
        </qweb>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="qoc_sales_order_delivery_schedule.action_window">
      <field name="name">Sales Order Delivery Schedule</field>
      <field name="res_model">sale.order.line</field>
      <field name="view_mode">qweb,tree,form,dashboard</field>
    </record>


